{
  "name": "jest-test",
  "version": "1.0.0",
  "description": "A basic config to run the Jest test tool.",
  "keywords": [
    "jest",
    "puppeteer",
    "axios",
    "tests",
    "e2e",
    "testing-tools",
    "commonjs",
    "esmodules"
  ],
  "author": "Bruce Taylor",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "scripts": {
    "testUnit": "jest --testPathIgnorePatterns='e2e.test.js'",
    "testE2E": "jest",
    "test-watch": "jest --watch --testPathIgnorePatterns='e2e.test.js'"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "preset": "jest-puppeteer"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.0",
    "babel-jest": "^25.2.6",
    "jest": "^25.2.7",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^2.1.1"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
