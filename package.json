{
  "name": "jest-test",
  "version": "1.0.0",
  "description": "A basic config to run the Jest test tool.",
  "keywords": [
    "jest",
    "puppeteer",
    "axios",
    "tests",
    "e2e",
    "testing-tools",
    "commonjs",
    "esmodules"
  ],
  "author": "Bruce Taylor",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "scripts": {
    "testUnit": "jest --testPathIgnorePatterns='e2e.test.js'",
    "testE2E": "jest",
    "test-watch": "jest --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.2",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "puppeteer": "^1.20.0"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
